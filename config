#!/bin/bash

VERSION=1.2.0-beta-001299-00
LIBUV_BRANCH=v1.10.2
COMMON_BRANCH=master
COREHOST_BRANCH=$COMMON_BRANCH
CORECLR_BRANCH=$COMMON_BRANCH
COREFX_BRANCH=$COMMON_BRANCH
ROSLYN_BRANCH=version-1.3.2
OS_NAME=Linux
OS_DIST=ubuntu
ROOTFS_BRANCH=xenial
ROOTFS_DIR=$(pwd)/rootfs/$ROOTFS_BRANCH
ARCH=arm
BUILD_CONFIG=Release
RUNTIME_ID=$OS_DIST.16.04-$ARCH
FILES_ROOT=$(pwd)/files
OUT_ROOT=$(pwd)/out
HOST_OUT=$OUT_ROOT/host/fxr/$VERSION
SHARED_OUT=$OUT_ROOT/shared/Microsoft.NETCore.App/$VERSION
CMAKE_C_COMPILER=$(which clang-3.5)
